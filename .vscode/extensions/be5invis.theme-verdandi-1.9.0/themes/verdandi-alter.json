{
	"$schema": "vscode://schemas/color-theme",
	"name": "Railgun",
	"palette": {
		"gray": [
			"#050607",
			"#111316",
			"#1b1f23",
			"#262b30",
			"#323940",
			"#424a52",
			"#5b646c",
			"#7e868d",
			"#a1a8ae",
			"#c0c5ca",
			"#d6dade"
		],
		"red": [
			"#0a0000",
			"#2b0000",
			"#4b0000",
			"#670000",
			"#7f0000",
			"#940000",
			"#a50404",
			"#b30e0e",
			"#c01c1c",
			"#cb2a2a",
			"#d53636"
		],
		"green": [
			"#041407",
			"#0d3c16",
			"#156323",
			"#1d842f",
			"#269d3a",
			"#30ae45",
			"#3cb951",
			"#4ac15f",
			"#59c86d",
			"#68ce7b",
			"#75d488"
		],
		"blue": [
			"#000f17",
			"#03314b",
			"#05507c",
			"#0866a3",
			"#0b73be",
			"#0f77cd",
			"#187ad5",
			"#2582db",
			"#368cdf",
			"#4696e3",
			"#549ee7"
		],
		"stress": [
			"#000f17",
			"#03314b",
			"#05507c",
			"#0866a3",
			"#0b73be",
			"#0f77cd",
			"#187ad5",
			"#2582db",
			"#368cdf",
			"#4696e3",
			"#549ee7"
		]
	},
	"colors": {
		"focusBorder": "#0f77cd",
		"foreground": "#a1a8ae",
		"errorForeground": "#a50404",
		"editor.background": "#050607",
		"editor.foreground": "#a1a8ae",
		"editor.lineHighlightBackground": "#111316",
		"editorCursor.foreground": "#0f77cd",
		"editorLineNumber.foreground": "#424a52",
		"editorActiveLineNumber.foreground": "#d6dade",
		"editor.selectionBackground": "#05507cff",
		"editor.selectionHighlightBackground": "#03314bff",
		"editor.inactiveSelectionBackground": "#262b30ff",
		"debugToolBar.background": "#1b1f23ff",
		"editorWidget.background": "#1b1f23ff",
		"editorSuggestWidget.background": "#050607ff",
		"editorGroup.border": "#262b30",
		"editorGroupHeader.noTabsBackground": "#050607",
		"editorGroupHeader.tabsBackground": "#1b1f23",
		"tab.border": "#262b30",
		"tab.inactiveBackground": "#1b1f23",
		"tab.inactiveForeground": "#7e868d",
		"tab.activeBackground": "#050607",
		"tab.activeForeground": "#d6dade",
		"peekViewEditor.background": "#262b3055",
		"peekViewTitle.background": "#050607ff",
		"peekView.border": "#262b30",
		"scrollbar.shadow": "#0000001a",
		"scrollbarSlider.background": "#00000013",
		"scrollbarSlider.activeBackground": "#00000026",
		"scrollbarSlider.hoverBackground": "#00000026",
		"editorOverviewRuler.border": "#00000000",
		"editorGutter.modifiedBackground": "#0f77cd",
		"editorGutter.addedBackground": "#30ae45",
		"editorGutter.deletedBackground": "#940000",
		"diffEditor.removedTextBackground": "#94000026",
		"diffEditor.insertedTextBackground": "#30ae451a",
		"sideBarTitle.foreground": "#d6dade",
		"sideBar.background": "#111316",
		"sideBarSectionHeader.background": "#262b30",
		"list.highlightForeground": "#187ad5",
		"list.hoverBackground": "#262b30",
		"list.inactiveSelectionBackground": "#424a524d",
		"list.activeSelectionBackground": "#0f77cd40",
		"list.focusBackground": "#0f77cd40",
		"list.inactiveSelectionForeground": "#d6dade",
		"list.activeSelectionForeground": "#d6dade",
		"list.focusForeground": "#d6dade",
		"gitDecoration.modifiedResourceForeground": "#368cdf",
		"gitDecoration.untrackedResourceForeground": "#59c86d",
		"gitDecoration.deletedResourceForeground": "#c01c1c",
		"gitDecoration.conflictingResourceForeground": "#c01c1c",
		"dropdown.background": "#050607",
		"dropdown.border": "#262b30",
		"dropdown.foreground": "#a1a8ae",
		"input.background": "#050607",
		"input.border": "#262b30",
		"input.foreground": "#a1a8ae",
		"input.placeholderForeground": "#424a52",
		"button.background": "#187ad5",
		"button.foreground": "#000f17",
		"button.hoverBackground": "#2582db",
		"badge.background": "#0f77cd",
		"badge.foreground": "#1b1f23",
		"activityBar.background": "#323940",
		"activityBar.foreground": "#c0c5ca",
		"statusBar.background": "#262b30",
		"statusBar.foreground": "#a1a8ae",
		"statusBar.noFolderBackground": "#262b30",
		"statusBar.noFolderForeground": "#a1a8ae",
		"statusBar.debuggingBackground": "#262b30",
		"statusBar.debuggingForeground": "#a1a8ae",
		"panel.border": "#262b30",
		"panelTitle.activeBorder": "#0f77cd",
		"titleBar.activeBackground": "#a1a8ae",
		"titleBar.activeForeground": "#111316",
		"titleBar.inactiveBackground": "#5b646c",
		"titleBar.inactiveForeground": "#1b1f23",
		"breadcrumb.foreground": "#5b646c",
		"breadcrumb.focusForeground": "#7e868d",
		"breadcrumb.activeSelectionForeground": "#7e868d",
		"breadcrumbPicker.background": "#111316"
	},
	"tokenColors": [
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#a1a8ae"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#a1a8ae"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#5b646c"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d6dade"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"delimiter",
				"bracket",
				"brace",
				"paren",
				"delimiter.tag",
				"punctuation.tag",
				"tag.html",
				"tag.xml",
				"meta.property-value punctuation.separator.key-value",
				"punctuation.definition.metadata.md",
				"string.link.md",
				"meta.brace"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#7e868d"
			}
		},
		{
			"name": "JavaScript string interpolation ${}",
			"scope": [
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.begin.ts",
				"punctuation.definition.template-expression.end.ts",
				"punctuation.definition.template-expression.end.js",
				"punctuation.section.embedded.begin.metatag.php",
				"punctuation.section.embedded.end.metatag.php"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#d6dade"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"meta.property-value.string",
				"support.constant.property-value.string",
				"meta.structure.dictionary.value.json string.quoted.double.json",
				"meta.structure.dictionary.json string.quoted.double.json",
				"meta.preprocessor string"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7e868d"
			}
		},
		{
			"name": "Primitive Literals",
			"scope": [
				"constant.numeric",
				"meta.property-value.numeric",
				"support.constant.property-value.numeric",
				"meta.property-value.color",
				"support.constant.property-value.color",
				"constant.language"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#d6dade"
			}
		},
		{
			"name": "User names",
			"scope": [
				"constant.character",
				"constant.other",
				"entity.name.function",
				"entity.name.class",
				"entity.other.inherited-class",
				"entity.other.attribute-name",
				"entity.name",
				"entity.other.attribute-name",
				"entity.other.attribute-name.html",
				"support.type.property-name",
				"entity.name.tag.table",
				"meta.structure.dictionary.json string.quoted.double.json"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#d6dade"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"meta.property-value.keyword",
				"support.constant.property-value.keyword",
				"meta.preprocessor.keyword",
				"keyword.other.use",
				"keyword.other.function.use",
				"keyword.other.namespace",
				"keyword.other.new",
				"keyword.other.special-method",
				"keyword.other.unit",
				"keyword.other.use-as"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d6dade"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage",
				"storage.type",
				"storage.type.ts",
				"storage.type.var.ts",
				"storage.type.js",
				"storage.type.var.js",
				"storage.type.const.ts",
				"storage.type.let.ts",
				"storage.type.let.js",
				"storage.type.const.js",
				"entity.name.tag"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d6dade"
			}
		},
		{
			"name": "Pointer, access, etc",
			"scope": [
				"meta.ptr",
				"meta.pointer",
				"meta.address",
				"meta.array.cxx"
			],
			"settings": {}
		},
		{
			"name": "Preprocessor",
			"scope": "meta.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#d6dade"
			}
		},
		{
			"name": "Library",
			"scope": [
				"support.type",
				"support.class",
				"support.function",
				"support.constant"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#d6dade"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#b30e0e"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#b30e0e"
			}
		},
		{
			"name": "Markdown Title Hash",
			"scope": [
				"punctuation.definition.heading.md",
				"entity.name.type.md",
				"beginning.punctuation"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#d6dade"
			}
		},
		{
			"name": "Markdown titles",
			"scope": [
				"markup.heading",
				"entity.name.section"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d6dade"
			}
		},
		{
			"name": "Markdown Raw",
			"scope": [
				"markup.raw",
				"markup.inline.raw",
				"markup.fenced",
				"markup.fenced_code"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7e868d"
			}
		},
		{
			"name": "Markdown link",
			"scope": [
				"markup.link",
				"string.other.link.title",
				"string.other.link.description",
				"meta.link.inline",
				"meta.image.inline"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#d6dade"
			}
		},
		{
			"name": "Makefile Variables",
			"scope": [
				"variable.language.makefile",
				"variable.other.makefile"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#d6dade"
			}
		},
		{
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "CSS Class",
			"scope": [
				"entity.other.attribute-name.class.css"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#d6dade"
			}
		},
		{
			"name": "CSS Tag name",
			"scope": [
				"entity.name.tag.css"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d6dade"
			}
		},
		{
			"name": "CSS Property",
			"scope": [
				"meta.property-name.css"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#d6dade"
			}
		}
	]
}